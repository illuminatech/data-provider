{
    "name": "illuminatech/data-provider",
    "description": "Allows easy build for DB queries from API requests",
    "keywords": ["laravel", "data", "provider", "query", "builder", "rest", "api"],
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/illuminatech/data-provider/issues",
        "wiki": "https://github.com/illuminatech/data-provider/wiki",
        "source": "https://github.com/illuminatech/data-provider"
    },
    "authors": [
        {
            "name": "Paul Klimov",
            "email": "klimov.paul@gmail.com"
        }
    ],
    "require": {
        "illuminate/support": "^5.8 || ^6.0 || ^7.0 || ^8.0 || ^9.0 || ^10.0"
    },
    "require-dev": {
        "illuminate/database": "*",
        "illuminate/http": "*",
        "illuminate/pagination": "*",
        "illuminate/config": "*",
        "phpunit/phpunit": "^7.5 || ^8.0 || ^9.3 || ^10.0.7"
    },
    "autoload": {
        "psr-4": {
            "Illuminatech\\DataProvider\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Illuminatech\\DataProvider\\Test\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        },
        "laravel": {
            "providers": [
                "Illuminatech\\DataProvider\\DataProviderServiceProvider"
            ]
        }
    }
}